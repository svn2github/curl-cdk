{curl 5.0 applet}
{applet manifest = "../manifest.mcurl",
    {compiler-directives careful? = true}
}

{value
    let locs:UrlArray =
        {UrlArray
            {url "t-cbi.curl"},
            {url "t-foodmart.curl"},
            {url "t-census.curl"},
            {url "t-census-memory.curl"},
            {url "t-callback.curl"},
            {url "t-function.curl"},
            {url "t-function-generic.curl"},
            || EXPERIMENTAL DATA-ACCESS alternatives
            {url "t-statement-dbc.curl"},
            {url "t-table-sync.curl"}
        }
    let display:VBox = 
        {VBox spacing = 3px}
    {for loc in locs do
        {display.add
            {link href = loc, target = "TEST",
                {value loc.filename}}}}
    display
}
