{define-class public PendingSend
  field public constant agent:#MessageAgent
  field public constant message:#IMessage

  {constructor public {default
                          agent:#MessageAgent,
                          message:#IMessage
                      }
    set self.agent = agent
    set self.message = message
  }
}
